{
  "displayName": "GPT Chat SDK",
  "version": "1.0.3",
  "name": "gpt-chat-sdk",
  "description": "Unofficial typed SDK for interacting with the OpenAI Chat Api",
  "scripts": {
    "copy_docs_from_root": "cp ../../README.md ./",
    "copy_docs_from_root_to_dist": "cp ../../LICENCE ../../dist/packages/gpt-chat-sdk;cp ../../COPYRIGHT ../../dist/packages/gpt-chat-sdk;cp ../../THIRD_PARTY_LICENCES ../../dist/packages/gpt-chat-sdk;",
    "build": "npm run copy_docs_from_root;cd ../../ && nx run gpt-chat-sdk:build; cd packages/gpt-chat-sdk && npm run copy_docs_from_root_to_dist"
  },
  "type": "commonjs",
  "repository": "https://github.com/doc-packages/gpt-chat-sdk",
  "license": "GPL",
  "author": "Francesco Bellini",
  "contributors": [
    "Francesco Bellini (@doc-packages)"
  ],
  "keywords": ["chatgpt", "chatgpt-api", "gpt", "gpt-3.5-turbo", "openai", "openai-api", "gpt-3", "gpt-client", "gpt-sdk", "openai chat", "chat", "ai"],
  "dependencies": {
    "axios": "^1.3.4"
  }
}
